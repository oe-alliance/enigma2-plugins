msgid "%i ms"
msgstr "%i جزء من الثانية"

msgid "Audio Sync"
msgstr "مزامنة الصوت"

msgid "Audio Sync Setup"
msgstr "ضبط مزامنة الصوت"

msgid "Cancel"
msgstr "إلغاء"

msgid "Center screen at the lower border"
msgstr "منتصف الشاشة إلى الحد السفلي"

msgid "Center screen at the upper border"
msgstr "منتصف الشاشة إلى الحد الأعلى"

msgid "Change active delay"
msgstr "تغيير التأخير النشط"

msgid "Channel audio:"
msgstr "صوت القناة:"

msgid "Decrease delay"
msgstr "قلل التأخير"

msgid "Decrease delay by %i ms (can be set)"
msgstr "قلل التأخير بـ %i مللى ثانيه (يمكن إعدادها)"

msgid "Discard changes and close plugin"
msgstr "تجاهل التغيرات وغلق البلج إنز"

msgid "Discard changes and close screen"
msgstr "تجاهل التغيرات وأغلاق الشاشه"

msgid "Global delay"
msgstr "تأخير عالمي"

msgid "Increase delay"
msgstr "زيادة التأخير"

msgid "Increase delay by %i ms (can be set)"
msgstr "زيادة التأخير بمقدار %i مللى ثانيه (يمكن الضبط)"

msgid "Invalid selection"
msgstr "إختيار غير صالح"

msgid "Key %(Key)s successfully set to %(delay)i ms"
msgstr "مفتاح % (مفاتيح) تم الضبط بنجاح %(تأخير) جزء من الثانية"

msgid "Key %(key)s (current value: %(value)i ms)"
msgstr "مفتاح % (مفاتيح) القيمة الحالية %(قيمة) جزء من الثانية."

msgid "Menu"
msgstr "قائمة"

msgid "Move plugin screen"
msgstr "حرك شاشة البلج إنز"

msgid "Move screen down"
msgstr "تحريك الشاشه للأسفل"

msgid "Move screen to the center of your TV"
msgstr "تحريك الشاشه الى منتصف التليفزيون"

msgid "Move screen to the left"
msgstr "تحريك شاشة العرض إلى اليسار"

msgid "Move screen to the lower left corner"
msgstr "حرك الشاشه إلى الركن الايسر السفلي"

msgid "Move screen to the lower right corner"
msgstr "حرك الشاشه إلى الركن الايمن السفلي"

msgid "Move screen to the middle of the left border"
msgstr "حرك الشاشه إلى منتصف الحافه اليسرى"

msgid "Move screen to the middle of the right border"
msgstr "حرك الشاشه إلى منتصف الحافه اليمنى"

msgid "Move screen to the right"
msgstr "حرك الشاشه إلى اليمين"

msgid "Move screen to the upper left corner"
msgstr "حرك الشاشه إلى الركن الايسر العلوي"

msgid "Move screen to the upper right corner"
msgstr "حرك الشاشه إلى الركن الايمن العلوي"

msgid "Move screen up"
msgstr "حرك الشاشه للأعلى"

msgid "OK"
msgstr "موافق"

msgid "Open plugin menu"
msgstr "أفتح قائمة البلج إنز"

msgid "Outer Bound (+/-)"
msgstr "الالتزام الخارجي (+/)"

msgid "Plugin: %(plugin)s , Version: %(version)s"
msgstr "البرنامج المساعد: s % (البرنامج المساعد)، الإصدار: s % (الإصدار)"

msgid "Reset saved position"
msgstr "إعادة المواقع المحفوظة للوضع الإفتراضي"

msgid "Save current delay to key"
msgstr "حفظ التأخير الحالي فى مفتاح"

msgid "Save to key"
msgstr "حفظ فى مفتاح"

msgid "Save values and close plugin"
msgstr "حفظ القيم وإغلاق البلج إن"

msgid "Save values and close screen"
msgstr "حفظ القيم وإغلاق الشاشة"

msgid "Select the key you want to set to %i ms"
msgstr "أختار المفتاح الذى تريد الضبط إلى %i جزء من الثانيه"

msgid "Service delay"
msgstr "تأخير الخدمة"

msgid "Set delay to %i ms (can be set)"
msgstr "أضبط التاخير الى %i جزء من الثانيه (يمكن ضبطها)"

msgid "Setting key canceled"
msgstr "تم الغاء مفاتيح الاعداد"

msgid "Setup for the Audio Sync Plugin"
msgstr "إعدادات بلجن أنز مزامنة الصوت"

msgid "Step in ms for arrow keys"
msgstr "خطوة جزء من الثانية للمفاتيح الأسهم"

msgid "Step in ms for key %i"
msgstr "خطوة جزء من الثانية لمفتاح ٪i"

msgid "Step in ms for keys '%s'"
msgstr "خطوة جزء من الثانية لمفاتيح '%s'"

msgid "Wait time in ms before activation:"
msgstr "أنتظر أجزاء من الثانية قبل التفعيل:"

msgid "sets the Audio Delay (LipSync)"
msgstr "ضبط تأخير الصوت (مزامنة الصوت مع حركة الشفايف)"
